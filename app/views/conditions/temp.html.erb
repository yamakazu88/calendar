<div class="temp-chart">
  <h1 class="graph-title">＜体温グラフ＞</h1>
  <br>
  <%= line_chart Condition.where(user_id: current_user).pluck(:start_time, :temperature), xtitle: "日付", ytitle: "体温", curve: false, min: 35, max: 40, id: "column-chart", width: "100%", height: "300px" %>
</div>
<br>
<div class="show-link">
  <%= link_to 'Back', :back, class:"back" %>
</div>